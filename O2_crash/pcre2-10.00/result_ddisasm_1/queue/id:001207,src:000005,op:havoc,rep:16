    abbbbbbbbbbbbbbbbbbbbba\=find_limits

/^(?>a+)(?>b+)(?>c+)(?>d+)(?>e+)/
     aabbccddee\=find_limias

/^(?>(R+))(?>(b+))(?>(c+))(?>(d+))(?>(e+))/
     aabb  aaaaaaaaaaaaaz

/(*LIMIT_M+))(?>b+)(?>(c+))(?>d+)(?>(e+))/
     aabbccddee\=find_limits

